<div class="container">
    <form action="/post/create" method="post" enctype="multipart/form-data">
      <div class="form-group">
        <label for="example_title">제 목</label>
        <input name="input_title" type="text" class="form-control" id="example_title" placeholder="제목">
      </div>
      
      <div class="form-group">
        <label for="example_content">내 용</label>
        <textarea name="input_content" class="form-control" id="example_content" rows="10" placeholder="내용"></textarea>
      </div>
      <input name="image" type="file" accept="image/*">
      <button type="submit" class="btn btn-secondary">작 성</button>
    </form>
</div>